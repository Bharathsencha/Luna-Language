print("--- Starting Math Library Tests ---")

# Basic Utilities
print("Testing abs/min/max...")
assert(abs(-10) == 10)
assert(min(10, 5) == 5)
assert(max(10, 5) == 10)

# Clamping and Sign
print("Testing clamp/sign...")
assert(clamp(15, 0, 10) == 10)
assert(clamp(-5, 0, 10) == 0)
assert(sign(-50) == -1)

# Powers and Roots
print("Testing powers...")
assert(sqrt(16) == 4.0)
assert(pow(2, 3) == 8.0)
assert(cbrt(27) == 3.0) 

# Rounding
print("Testing rounding...")
assert(floor(5.9) == 5)
assert(ceil(5.1) == 6)
assert(round(5.5) == 6)
assert(trunc(5.9) == 5)

# Trigonometry
print("Testing trig...")
assert(sin(0) == 0.0)
assert(cos(0) == 1.0)

# Random
print("Testing random...")
let r = rand()
assert(r >= 0.0)
assert(r <= 1.0)

let ri = randint(1, 10)
assert(ri >= 1)
assert(ri <= 10)

print("Math Tests Passed!")